<html>
<head>
  <title>Chat</title>
  <link rel="stylesheet" href="/css/styleMain.css">
</head>
<body>
  <div id="main">
    <div id="menu"></div>
    <div id="room">
      <div id="header_room">
          <h1 id="h1header">Room List</h1>
      </div>
      <div id="room_list">
        <% lists.forEach(function(item) { %>
          <div id="room_list div"> <%= item.room_id %></div>
        <% }); %>
      </div>
    </div>
    <div id="chat">
      <div id="chat_parent">
        <div id="header_chat">
          <h1 id="header_agent"></h1>
        </div>
        <div id="messages_agent"></div>
        <div id="footer">
          <form id="form_agent" name="form_agent" action="">
            <input id="input_agent" autocomplete="off"/>
            <input type="hidden" name="user_type" id="user_type" value="agent" />
            <input type="hidden" name="room_id_agent" id="room_id_agent" />
            <input type="hidden" name="agent_id_agent" id="agent_id_agent" />
            <input type="submit" id="btn_send_agent" value="Send" />
            <input type="submit" id="btn_done_agent" value="Done" />
          </form>
        </div>
      </div>
    </div>
  </div>


  <script src="https://code.jquery.com/jquery-3.1.1.slim.js" integrity="sha256-5i/mQ300M779N2OVDrl16lbohwXNUdzL/R2aVUXyXWA="   crossorigin="anonymous"></script>
  <script src="/js/entry.js" type="text/javascript"></script>
</body>
</html>
