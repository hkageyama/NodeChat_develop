<html>
<head>
  <title>Chat</title>
  <link rel="stylesheet" href="/css/styleEntry.css">
</head>
<body>
  <div id="content">
    <div id="header">
      <h1 id="h1header">Chat Room</h1>
    </div>
    <div id="create" style="display:inline-flex">
      <form id="entryFormAgent" style="display: inline" method="post" action="/agent" target="_blank">
        <input type="hidden" name="room_id" id="room_id"/>
        <input type="hidden" name="agent_id" id="agent_id"/>
        <input type="submit" id="btn_create_agent" value="create room agent" />
      </form>
      <form id="entryFormVisitor" style="display: inline" method="post" action="/visitor" target="_blank">
        <input type="submit" id="btn_create_visitor" value="create room visitor" />
      </form>
    </div>
    <div id="select">
      <div id="room-list">
          <% lists.forEach(function(item) { %>
            <div id="room-list div"> <%= item.room_id %>  : <%= item.agent_id %></div>
          <% }); %>
      </div>
    </div>
  </div>
  <script src="https://code.jquery.com/jquery-3.1.1.slim.js" integrity="sha256-5i/mQ300M779N2OVDrl16lbohwXNUdzL/R2aVUXyXWA="   crossorigin="anonymous"></script>
  <script src="/js/entry.js" type="text/javascript"></script>
</body>
</html>