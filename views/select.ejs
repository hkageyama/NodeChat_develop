<html>
<head>
  <title>Chat</title>
  <link rel="stylesheet" href="/css/styleEntry.css">
</head>
<body>
  <div id="content">
    <div id="header">
      <h1 id="h1header">Chat Room</h1>
    </div>
    <div id="create">
      <form id="entryForm" method="post" action="/agent">
        <input type="hidden" name="room_id" id="room_id"/>
        <input type="hidden" name="agent_name" id="agent_name"/>
        <input type="submit" id="btn_create" value="create room" />
      </form>
    </div>
    <div id="select">
      <div id="room-list">
          <% lists.forEach(function(item) { %>
            <div id="room-list div"> <%= item.room_id %>  : <%= item.visitor_id %></div>
          <% }); %>
      </div>
    </div>
  </div>
  <script src="https://code.jquery.com/jquery-3.1.1.slim.js" integrity="sha256-5i/mQ300M779N2OVDrl16lbohwXNUdzL/R2aVUXyXWA="   crossorigin="anonymous"></script>
  <script src="/js/entry.js" type="text/javascript"></script>
</body>
</html>