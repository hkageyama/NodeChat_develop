<!doctype html>
<html>
  <head>
    <title>Socket.IO chat</title>
    <link rel="stylesheet" href="/css/style.css">
  </head>
  <body id="body_visitor">
    <div id="content">
      <div id="header">
        <div id="header_visitor"><%= agentId %> @ Room #<%= roomId %></div>
      </div>
      <div id="messages_visitor"></div>
      <div id="footer">
        <form id="form_visitor" action="">
          <input id="input_visitor" autocomplete="off"/>
          <input type="hidden" name="user_type" id="user_type" value="visitor" />
          <input type="hidden" name="room_id_visitor" id="room_id_visitor" value=<%= roomId %> />
          <input type="hidden" name="agent_id_visitor" id="agent_id_visitor" value=<%= agentId %> />
          <input type="submit" id="btn_send_visitor" value="Send" />
        </form>
      </div>
    </div>
    <script src="/socket.io/socket.io.js"></script>
    <script src="https://code.jquery.com/jquery-3.1.1.slim.js" integrity="sha256-5i/mQ300M779N2OVDrl16lbohwXNUdzL/R2aVUXyXWA="   crossorigin="anonymous"></script>
    <script src="/js/chat.js" type="text/javascript"></script>
  </body>
</html>
